{
    "name": "touhidurabir/laravel-presenter",
    "description": "A package that helps to group methods that mostly use for the view presentation purpose.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Touhidurabir\\Presenter\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Touhidurabir\\Presenter\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Touhidur Rahman",
            "email": "abircse06@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.21"
    },
    "require": {
        "php": ">=7.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Touhidurabir\\Presenter\\PersenterServiceProvider"
            ]
        }
    }
}
